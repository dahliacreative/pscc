{
  "name": "concrete5/documentation_generator",
  "description": "concrete5 composer package for adding commands that generate documentation.",
  "version": "1.0-beta3",
  "minimum-stability": "dev",
  "bin-dir": "/bin",
  "require": {
    "phpdocumentor/reflection-docblock": "~2.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mlocati/zendframework2-i18n-v3"
    },
    {
      "type": "vcs",
      "url": "https://github.com/concrete5/flysystem"
    },
    {
      "type": "vcs",
      "url": "https://github.com/korvinszanto/concrete5-composer"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "concrete5/concrete5": "dev-develop"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Korvin Szanto",
      "email": "korvinszanto@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Concrete\\DocumentationGenerator\\": "src/"
    }
  }
}
